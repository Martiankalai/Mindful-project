    "def mirror(str,N):\n",
    "   str.lower()\n",
    "   result=list(str)\n",
    "   \n",
    "   for i in range(N,len(result)):\n",
    "       char=result[i]\n",
    "       if char.isalpha():\n",
    "           mirrored_char=chr(219-ord(char))\n",
    "           result[i]=mirrored_char\n",
    "   return result\n",
    "\n",
    "str=\"paradox\"\n",
    "N=3\n",
    "print(mirror(str,N))\n",
    "   "
   ]
  }
