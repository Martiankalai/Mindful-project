    "arr=[1,3,7,8,11]\n",
    "k=15\n",
    "def twosum(arr,k):\n",
    "    twodict={}\n",
    "    n=len(arr)\n",
    "    for i in range(n):\n",
    "        if k-arr[i] in twodict:\n",
    "            return [twodict.get(k-arr[i]),i]\n",
    "        else:\n",
    "            twodict[arr[i]]=i\n",
    "            print(twodict)\n",
    "   \n",
    "    return\n",
    "print(twosum(arr,k))\n",
    "    "
