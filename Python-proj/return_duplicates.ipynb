   "source": [
    "arr=[12,1,3,4,2,1,2]\n",
    "def duplicate(arr):\n",
    "    seen=set()\n",
    "    dupl=set()\n",
    "    for ele in arr:\n",
    "        if ele in seen:\n",
    "            dupl.add(ele)\n",
    "        else:\n",
    "            seen.add(ele)\n",
    "    if dupl:\n",
    "        return sorted(list(dupl))\n",
    "    else:\n",
    "        return -1\n",
    "print(duplicate(arr))"
   ]
  }
